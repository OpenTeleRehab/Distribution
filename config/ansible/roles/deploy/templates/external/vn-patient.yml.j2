---

version: '3'
networks:
  hiv-docker-net:

volumes:
  vn_patient_service_vol:

services:
  vn_patient_service:
    container_name: vn_patient_service
    image: {{ image_name }}/{{ stage }}/patient-service:{{ version }}
    restart: unless-stopped
    ports:
      - "80:80"
    networks:
      - hiv-docker-net
    volumes:
      - vn_patient_service_vol:/var/www/storage
      - ./config/vn_patient/env:/var/www/.env
