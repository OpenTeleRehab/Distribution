---
apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: ing-{{ stage }}-{{ project_name }}-superset
  namespace: '{{ k8s_namespace }}'
spec:
  rules:
    - host: {{ superset_host }}
      http:
        paths:
          - path: "/"
            pathType: ImplementationSpecific
            backend:
              service:
                name: 'svc-{{ stage }}-{{ project_name }}-superset'
                port:
                  number: 8088
