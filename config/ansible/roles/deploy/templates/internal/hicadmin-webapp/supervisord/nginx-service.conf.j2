[program:nginx]
process_name=nginx
command=/usr/sbin/nginx -g "daemon off;"
autostart=true
autorestart=true
startretries=5
numprocs=1
startsecs=0
stdout_logfile=/var/log/supervisor/nginx-stdout
stdout_logfile_maxbytes=10MB
stderr_logfile=/var/log/supervisor/nginx-stderr
stderr_logfile_maxbytes=10MB
exitcodes=0
